                    <div class="content-wrap">
                        <div class="item-wrap">
                            <div class="img-item">
                                
                                <% if(user.photoUrl[0]) { %>
                                    <img src="<%= user.photoUrl[0] %>" class="user-img">
                                    
                                <% } else { %>
                                    <img src="./imges/avatar/Avatar=13.png" class="user-img">
                                <% } %>
                                <div class="fly-circle"></div>
                            </div>
    
                            <div class="title-wrap">
                                <div class="title"><%= user.username %></div>
                                <div class="email"><%= user.email %></div>
                            </div>
                        </div>
                        
                        <div class="item-wrap">
                            <div class="user-id-item">
                                <div class="user-item">
                                    <div class="title">승인여부</div>
                                    <div class="user-content">
                                        <% if(user.judge == 0 || user.judge == 1) { %>
                                            <input disabled type="text" placeholder="대기중"></input>
                                        <% } else if(user.judge == 2) { %>
                                            <input disabled type="text" placeholder="반려됨"></input>
                                        <% } else if(user.judge == 3) { %>
                                            <input disabled type="text" placeholder="승인됨"></input>
                                        <% } %>
                                        
                                    </div>
                                </div>
                                <div class="user-item">
                                    <div class="title">보석개수</div>
                                    <div class="user-content">
                                        <input type="text" id="dia" placeholder="0" value="<%= user.dia %>"></input>
                                        <button class="btn-plus">변경</button>
